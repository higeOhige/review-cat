{
  "name": "review-cat",
  "license": "MIT",
  "version": "0.0.0",
  "main": "./dist/app.js",
  "scripts": {
    "dev": "pnpm vite:dev & pnpm electron:dev",
    "build": "pnpm clean && pnpm vite:build && pnpm copy && pnpm electron:build",
    "clean": "pnpm clean:app & pnpm clean:dist",
    "serve": "vite preview",
    "vite:dev": "vite",
    "vite:build": "tsc && NODE_ENV=production vite build --emptyOutDir",
    "electron:dev": "NODE_ENV=development electron ./dev/electron.js",
    "electron:build": "node build.js",
    "clean:app": "rimraf app",
    "clean:dist": "rimraf dist",
    "copy": "cpx dev/app.js dist"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "cpx": "^1.5.0",
    "electron": "^13.1.5",
    "electron-builder": "^22.11.7",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.0-beta",
    "vite": "^2.3.5"
  }
}
